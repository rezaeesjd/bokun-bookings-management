{
    "name": "bokun/bookings-management",
    "description": "WordPress plugin to manage Bokun bookings",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Bokun\\Bookings\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.9",
        "wp-coding-standards/wpcs": "^3.0"
    },
    "scripts": {
        "lint:phpcs": [
            "@php vendor/bin/phpcs"
        ]
    }
}
